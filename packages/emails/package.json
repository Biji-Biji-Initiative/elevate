{
  "name": "@elevate/emails",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/js/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "echo 'Email templates ready'",
    "build": "pnpm run build:types && tsup --config tsup.config.ts",
    "clean": "rm -rf dist",
    "export": "echo 'Email templates exported'",
    "build:types": "tsc -b tsconfig.build.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --fix",
    "type-check": "tsc --noEmit",
    "api:extract": "../../node_modules/.bin/api-extractor run --local",
    "api:check": "../../node_modules/.bin/api-extractor run"
  },
  "dependencies": {
    "@react-email/components": "^0.0.25",
    "@react-email/render": "^1.0.1",
    "resend": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.1.12",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-email": "^3.0.1",
    "typescript": "^5.9.2",
    "tsup": "^8.3.7"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.11"
  },
  "files": [
    "dist/js",
    "dist/types",
    "README.md",
    "LICENSE",
    "dist/js/**/*.map"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "publishConfig": {
    "access": "restricted",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/elevate.git",
    "directory": "packages/emails"
  }
}
