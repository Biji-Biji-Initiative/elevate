{
  "name": "@elevate/types",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=20.11"
  },
  "files": [
    "dist/js",
    "dist/types",
    "README.md",
    "LICENSE",
    "dist/js/**/*.map"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/js/index.js"
    },
    "./schemas": {
      "types": "./dist/types/schemas.d.ts",
      "default": "./dist/js/schemas.js"
    },
    "./query-schemas": {
      "types": "./dist/types/query-schemas.d.ts",
      "default": "./dist/js/query-schemas.js"
    },
    "./admin-schemas": {
      "types": "./dist/types/admin-schemas.d.ts",
      "default": "./dist/js/admin-schemas.js"
    },
    "./api-types": {
      "types": "./dist/types/api-types.d.ts",
      "default": "./dist/js/api-types.js"
    },
    "./ui-types": {
      "types": "./dist/types/ui-types.d.ts",
      "default": "./dist/js/ui-types.js"
    },
    "./common": {
      "types": "./dist/types/common.d.ts",
      "default": "./dist/js/common.js"
    },
    "./submission-payloads": {
      "types": "./dist/types/submission-payloads.d.ts",
      "default": "./dist/js/submission-payloads.js"
    },
    "./webhooks": {
      "types": "./dist/types/webhooks.d.ts",
      "default": "./dist/js/webhooks.js"
    },
    "./canonical-urls": {
      "types": "./dist/types/canonical-urls.d.ts",
      "default": "./dist/js/canonical-urls.js"
    },
    "./http": {
      "types": "./dist/types/http.d.ts",
      "default": "./dist/js/http.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/js/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "scripts": {
    "build": "pnpm run build:types && tsup --config tsup.config.ts",
    "build:types": "tsc -b --force tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "api:extract": "../../node_modules/.bin/api-extractor run --local",
    "api:check": "../../node_modules/.bin/api-extractor run"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.1.12",
    "tsup": "^8.3.7",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "restricted",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/elevate.git",
    "directory": "packages/types"
  }
}
