{
  "name": "@elevate/http",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=20.11"
  },
  "files": [
    "dist/js",
    "dist/types",
    "README.md",
    "LICENSE",
    "dist/js/**/*.map"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./error-utils": {
      "types": "./dist/types/error-utils.d.ts",
      "import": "./dist/js/error-utils.js"
    },
    "./middleware": {
      "types": "./dist/types/middleware.d.ts",
      "import": "./dist/js/middleware.js"
    },
    "./test-utils": {
      "types": "./dist/types/test-utils.d.ts",
      "import": "./dist/js/test-utils.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/js/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "scripts": {
    "build:js": "tsup --config tsup.config.ts",
    "build": "pnpm run build:types && pnpm run build:js",
    "build:types": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "api:extract": "../../node_modules/.bin/api-extractor run --local",
    "api:check": "../../node_modules/.bin/api-extractor run"
  },
  "dependencies": {
    "@elevate/types": "workspace:*",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "next": "^15.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.1.12",
    "tsup": "^8.3.7",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "restricted",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/elevate.git",
    "directory": "packages/http"
  }
}
