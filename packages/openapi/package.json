{
  "name": "@elevate/openapi",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./spec": {
      "types": "./dist/types/spec.d.ts",
      "import": "./dist/js/spec.js"
    },
    "./spec.json": "./dist/openapi.json",
    "./sdk": {
      "types": "./dist/types/sdk.d.ts",
      "import": "./dist/js/sdk.js"
    },
    "./schemas": {
      "types": "./dist/types/schemas.d.ts",
      "import": "./dist/js/schemas.js"
    },
    "./client": {
      "types": "./dist/types/client.d.ts",
      "import": "./dist/js/client.js"
    },
    "./examples": {
      "types": "./dist/types/examples.d.ts",
      "import": "./dist/js/examples.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/js/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/js",
    "dist/types",
    "README.md",
    "LICENSE",
    "dist/js/**/*.map",
    "dist/openapi.json"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run generate:all && pnpm run build:types && tsup --config tsup.config.ts",
    "generate": "tsx src/generator.ts",
    "generate:spec": "tsx src/generator.ts",
    "generate:client": "openapi-typescript dist/openapi.json -o src/client.ts",
    "generate:sdk": "tsx src/client-generator.ts",
    "generate:all": "pnpm run generate && pnpm run generate:sdk",
    "dev": "tsx watch src/generator.ts",
    "clean": "rm -rf dist api-reports/*.api.md",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --fix",
    "type-check": "tsc --noEmit",
    "build:types": "tsc -b tsconfig.build.json",
    "api:extract": "../../node_modules/.bin/api-extractor run --local",
    "api:check": "../../node_modules/.bin/api-extractor run",
    "test": "vitest --run"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "7.3.4",
    "@elevate/types": "workspace:*",
    "openapi3-ts": "^4.5.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "openapi-typescript": "^7.9.1",
    "tsup": "^8.3.7",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.11"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "publishConfig": {
    "access": "restricted",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/elevate.git",
    "directory": "packages/openapi"
  }
}
