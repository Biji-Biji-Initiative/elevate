{
  "name": "@elevate/config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./next": {
      "types": "./dist/types/next.d.ts",
      "import": "./dist/js/next.js"
    },
    "./env": {
      "types": "./dist/types/env.d.ts",
      "import": "./dist/js/env.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/js/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build:js": "tsup --config tsup.config.ts",
    "build": "pnpm run build:types && pnpm run build:js",
    "clean": "rm -rf dist",
    "build:types": "tsc -b tsconfig.build.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.cjs --fix",
    "type-check": "tsc --noEmit",
    "api:extract": "../../node_modules/.bin/api-extractor run --local",
    "api:check": "../../node_modules/.bin/api-extractor run"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.9.2",
    "tsup": "^8.3.7"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.11"
  },
  "files": [
    "dist/js",
    "dist/types",
    "README.md",
    "LICENSE",
    "dist/js/**/*.map"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "publishConfig": {
    "access": "restricted",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/elevate.git",
    "directory": "packages/config"
  }
}
