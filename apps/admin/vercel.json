{
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=elevate-admin",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile && pnpm db:generate",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}