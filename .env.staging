# =============================================================================
# MS Elevate LEAPS Tracker - Staging Environment Configuration
# =============================================================================
# This file contains staging-specific environment values.
# Values here override those in .env.defaults
# This file CAN be committed (no sensitive production data)
# =============================================================================

# Environment
NODE_ENV=production
VERCEL_ENV=staging

# Application URLs - Staging
NEXT_PUBLIC_SITE_URL=https://staging-leaps.mereka.org

# Database - Staging (placeholder - should be set in Vercel environment)
DATABASE_URL=postgresql://staging-user:staging-password@staging-host:5432/elevate_leaps_staging
DIRECT_URL=postgresql://staging-user:staging-password@staging-host:5432/elevate_leaps_staging

# Authentication - Staging (placeholder - should be set in Vercel environment)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_staging_placeholder
CLERK_SECRET_KEY=sk_staging_placeholder
CLERK_WEBHOOK_SECRET=whsec_staging_placeholder

# Supabase - Staging (placeholder - should be set in Vercel environment)
NEXT_PUBLIC_SUPABASE_URL=https://staging-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=staging_anon_key_placeholder
SUPABASE_SERVICE_ROLE_KEY=staging_service_role_key_placeholder

# Kajabi Integration - Staging (placeholder - should be set in Vercel environment)
KAJABI_WEBHOOK_SECRET=staging-webhook-secret
KAJABI_API_KEY=staging_kajabi_api_key
KAJABI_CLIENT_SECRET=staging_kajabi_client_secret
KAJABI_BASE_URL=https://academy.mereka.my/api
KAJABI_LEARN_TAGS=elevate-ai-1-completed,elevate-ai-2-completed

# Email Service - Staging (placeholder - should be set in Vercel environment)
RESEND_API_KEY=re_staging_placeholder

# Email templates - Staging specific
FROM_EMAIL=MS Elevate Staging <noreply-staging@leaps.mereka.org>
REPLY_TO_EMAIL=staging-support@leaps.mereka.org

# Staging specific settings
DEBUG=false
RATE_LIMIT_RPM=120
WEBHOOK_RATE_LIMIT_RPM=240

# Cron protection for staging
CRON_SECRET=staging_cron_secret_placeholder

# Optional services - Staging
OPENAI_API_KEY=sk-staging_openai_placeholder
SENTRY_DSN=https://staging-sentry@sentry.io/staging-project-id
